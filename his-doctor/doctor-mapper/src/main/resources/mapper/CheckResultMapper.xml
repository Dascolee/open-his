<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dasco.openhis.mapper.CheckResultMapper">

    <resultMap id="BaseResultMap" type="com.dasco.openhis.domain.CheckResult">
            <id property="cocId" column="coc_id" jdbcType="VARCHAR"/>
            <result property="checkItemId" column="check_item_id" jdbcType="INTEGER"/>
            <result property="checkItemName" column="check_item_name" jdbcType="VARCHAR"/>
            <result property="price" column="price" jdbcType="DECIMAL"/>
            <result property="resultMsg" column="result_msg" jdbcType="VARCHAR"/>
            <result property="resultImg" column="result_img" jdbcType="OTHER"/>
            <result property="patientId" column="patient_id" jdbcType="VARCHAR"/>
            <result property="patientName" column="patient_name" jdbcType="VARCHAR"/>
            <result property="resultStatus" column="result_status" jdbcType="CHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="createBy" column="create_by" jdbcType="VARCHAR"/>
            <result property="updateBy" column="update_by" jdbcType="VARCHAR"/>
            <result property="regId" column="reg_id" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        coc_id,check_item_id,check_item_name,
        price,result_msg,result_img,
        patient_id,patient_name,result_status,
        create_time,update_time,create_by,
        update_by,reg_id
    </sql>
</mapper>
